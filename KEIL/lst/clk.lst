N */
N__STATIC_INLINE void UART_SET_RTS(UART_T *psUART)
Xstatic __inline void UART_SET_RTS(UART_T *psUART)
N{
N    psUART->MODEM |= UART_MODEM_RTSACTLV_Msk | UART_MODEM_RTS_Msk;
X    psUART->MODEM |= (0x1ul << (9)) | (0x1ul << (1));
N}

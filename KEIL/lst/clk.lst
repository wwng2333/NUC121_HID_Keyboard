N                        u32ChNum, \
N                        u32Condition, \
N                        u32Data, \
N                        u32MatchCount) ((adc)->ADCMPR[0] = ((u32ChNum) << ADC_ADCMPR_CMPCH_Pos) \
N                                        | (u32Condition) \
N                                        | ((u32Data) << ADC_ADCMPR_CMPD_Pos) \
N                                        | (((u32MatchCount) - 1) << ADC_ADCMPR_CMPMATCNT_Pos) \
N                                        | ADC_ADCMPR_CMPEN_Msk)
